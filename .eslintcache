 = observer(({ ticket }) => {\n\n    const [show, setShow] = useState(false);\n    const [success, setSuccess] = useState(false);\n\n    const currency = TicketStore.initialCurrency;\n    const currentPrice = Math.floor(ticket.price * currency.ratio);\n\n    const handleClose = () => {\n\n        setShow(false);\n        setSuccess(false);\n    };\n    const handleShow = () => setShow(true);\n\n    const formatDate = date => {\n\n        const options = { year: 'numeric', month: 'long', day: 'numeric', weekday: 'short' };\n        const newDate = new Date(date).toLocaleDateString(\"ru-RU\", options);\n        const slicedDate = newDate.split(',').slice(1);\n        const resultData = slicedDate.concat(newDate.split(',').slice(0,1)).join('').trim();\n\n        return resultData.replace(/\\./, \",\");\n    }\n\n    const changeSuccess = bool => {\n        setSuccess(bool)\n    }\n\n    return (\n        <>\n            <div className=\"item\">\n                <div className=\"item__left\">\n                    <img className=\"item__company-logo\" src={ logo } alt={ 'company' }/>\n                    {\n                        <OwnButton\n                            price={ `${currentPrice} ${currency.name}` }\n                            handleShow={ handleShow }\n                        />\n                    }\n\n                </div>\n                <div className=\"item__right\">\n                    <div className=\"time\">\n                        <p className=\"time__departure\">{ ticket.departure_time }</p>\n                        <p className=\"time__stops\">{ ticket.stops ? ticket.stops : 'Без' } пересадки</p>\n                        <p className=\"time__arrivals\">{ ticket.arrival_time }</p>\n                    </div>\n                    <div className=\"place\">\n                        <p className=\"place__from\">{ ticket.origin }, { ticket.origin_name }</p>\n                        <p className=\"place__to\">{ ticket.destination }, { ticket.destination_name }</p>\n                    </div>\n                    <div className=\"date\">\n                        <p className=\"date__from\">{formatDate(ticket.departure_date) }</p>\n                        <p className=\"date__to\">{formatDate(ticket.arrival_date) }</p>\n                    </div>\n                </div>\n            </div>\n            <Popup\n                success={ success }\n                changeSuccess={ changeSuccess }\n                isShow={ show }\n                handleClose={ handleClose }\n                data={ ticket }\n                price={ `${currentPrice} ${currency.name}` }/>\n        </>\n    )\n})\n\nexport default TicketItem;","/home/smiss/Desktop/1/aviasales/src/redux/actions/getCurrencyRates.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/types/loader.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/types/getTickets.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/types/filter.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/types/form.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/actions/filterStops.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/actions/isLoading.js",[],"/home/smiss/Desktop/1/aviasales/src/redux/actions/setFormData.js",[],"/home/smiss/Desktop/1/aviasales/src/components/OwnButton/OwnButton.js",[],"/home/smiss/Desktop/1/aviasales/src/components/Popup/Popup.js",[],"/home/smiss/Desktop/1/aviasales/src/components/FormComponent/FormComponent.js",[],"/home/smiss/Desktop/1/aviasales/src/components/Input/Input.js",[],"/home/smiss/Desktop/1/aviasales/src/Stores/ticketStore.js",[],"/home/smiss/Desktop/1/aviasales/src/Stores/formStore.js",[],{"ruleId":"251","replacedBy":"252"},{"ruleId":"253","replacedBy":"254"},{"ruleId":"251","replacedBy":"255"},{"ruleId":"253","replacedBy":"256"},{"ruleId":"251","replacedBy":"257"},{"ruleId":"253","replacedBy":"258"},{"ruleId":"259","severity":1,"message":"260","line":5,"column":10,"nodeType":"261","messageId":"262","endLine":5,"endColumn":17},{"ruleId":"259",